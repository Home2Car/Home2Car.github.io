    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.4.20",
    "javascript-obfuscator": "^4.1.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-go-template": "0.0.10",
    "tailwindcss": "^2.2.16",
    "path": "^0.12.7",
    "fs": "^0.0.1-security"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
